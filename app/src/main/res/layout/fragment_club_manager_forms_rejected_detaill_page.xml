                android:layout_height="175dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="32dp"
                android:autofillHints=""
                android:background="@drawable/normal_background"
                android:ems="10"
                android:fontFamily="@font/poppins"
                android:inputType="text"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView68" />

            <TextView
                android:id="@+id/textView69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/poppins"
                android:text="Event Goals"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_club_manager_forms_rejeected_detail_page_tw_content" />

            <TextView
                android:id="@+id/fragment_club_manager_forms_rejeected_detail_page_et_event_goals"
                android:layout_width="0dp"
                android:layout_height="90dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="32dp"
                android:autofillHints=""
                android:background="@drawable/normal_background"
                android:ems="10"
                android:fontFamily="@font/poppins"
                android:inputType="text"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView69" />

            <TextView
                android:id="@+id/textView70"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/poppins"
                android:text="Agenda"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_club_manager_forms_rejeected_detail_page_et_event_goals" />

            <EditText
                android:id="@+id/fragment_club_manager_forms_rejeected_detail_page_et_agenda"
